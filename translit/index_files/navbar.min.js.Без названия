document.addEventListener('DOMContentLoaded',function(){var $nb=Array.prototype.slice.call(document.querySelectorAll('.navbar-burger'),0);if($nb.length>0){$nb.forEach(function($el){$el.addEventListener('click',function(){var t=$el.dataset.target,$t=document.getElementById(t);$el.classList.toggle('is-active');$t.classList.toggle('is-active');});});}});
